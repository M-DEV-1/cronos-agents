{
  "name": "cronos-x402-agent-payment",
  "version": "1.0.0",
  "description": "Standard X402 protocol implementation with Cronos X402 facilitator",
  "main": "index.js",
  "scripts": {
    "start:agent-b": "npx ts-node agent-b-server.ts",
    "start:agent-a": "npx ts-node agent-a-client.ts",
    "start:a2ui": "npx ts-node src/a2ui-server.ts",
    "start:mcp": "npx ts-node src/source-mcp-server.ts",
    "start:music": "npx ts-node src/music-mcp-server.ts",
    "dev": "concurrently \"npm run start:agent-b\" \"npm run start:agent-a\"",
    "dev:full": "concurrently \"npm run start:a2ui\" \"npm run start:mcp\" \"npm run start:music\"",
    "build": "tsc",
    "test": "echo \"No tests specified\""
  },
  "keywords": [
    "cronos",
    "x402",
    "agent-to-agent",
    "payments",
    "blockchain",
    "ai-agents"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "ethers": "^6.9.0",
    "express": "^4.18.0",
    "viem": "^1.21.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/node": "^20.0.0",
    "concurrently": "^8.2.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
